{
  "name": "disc-electron",
  "version": "0.0.1",
  "description": "An Electron version of the DISC application.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "install-app-deps",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "repository": "",
  "author": "",
  "license": "",
  "devDependencies": {
    "electron": "4.0.3",
    "electron-builder": "^20.39.0"
  },
  "dependencies": {
    "d3-tip": "^0.9.1",
    "intro.js": "^2.9.3",
    "jquery-slinky": "^4.1.0",
    "papaparse": "^4.6.3",
    "sqlite3": "^4.0.6",
    "wkhtmltopdf": "^0.3.4"
  },
  "build": {
    "asar": true,
    "appId": "disc.app",
    "productName": "Decision Integration for Strong Communities",
    "mac": {
      "category": "disc.app"
    },
    "nsis": {
      "oneClick": false
    },
    "extraResources": [
      "hwbi_app/DISC.db"
    ]
  }
}
